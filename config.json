{
  "type": "network",
  "version": "1.0",
  "data": "data.json",
  "logo": {
    "file": "https://raw.githubusercontent.com/sclipman/SociospatialNet/master/images/spatial.png",
    "link": "",
    "text": "Zixi Liu"
  },
  "text": {
    "more": "Created by: Steven J. Clipman, MSPH | PhD candidate at Johns Hopkins University",
    "intro": "This is an interactive sociospatial network visualization of patients from Penn Medicine. Edges represent a social connection, i.e. self-reported partners.",
    "title": "Penn Medicine Obesity Sociospatial Network"
  },
  "legend": {
    "edgeLabel": "Edge = Social/Spatial Connection",
    "colorLabel": "Blue = Prevalent HCV Monoinfection; Red = Prevalent HIV Monoinfection; Yellow = Prevalent HIV/HCV Coinfection; Grey = HIV/HCV Negative; Purple = Spatial Hotspot",
    "nodeLabel": "Node = Individual or Hotspot"
  },
  "features": {
    "search": true,
    "groupSelectorAttribute": "color",
    "hoverBehavior": "dim"
  },
  "informationPanel": {
    "groupByEdgeDirection": false,
    "imageAttribute": false
  },
  "sigma": {
    "drawingProperties": {
      "defaultEdgeType": "curve",
      "defaultHoverLabelBGColor": "#002147",
      "defaultLabelBGColor": "#ddd",
      "activeFontStyle": "bold",
      "defaultLabelColor": "#000",
      "labelThreshold": 10,
      "defaultLabelHoverColor": "#fff",
      "fontStyle": "bold",
      "hoverFontStyle": "bold",
      "defaultLabelSize": 14
    },
    "graphProperties": {
      "maxEdgeSize": 1.5,
      "minEdgeSize": 1.5,
      "minNodeSize": 1,
      "maxNodeSize": 7
    },
    "mouseProperties": {
      "maxRatio": 20,
      "minRatio": 0.75
    }
  }
}
